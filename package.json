{
  "name": "agapequiz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "seed": "tsx prisma/seed.ts",
    "clean": "rimraf .next out",
    "type-check": "tsc --noEmit",
    "prebuild": "npm run type-check && npm run lint",
    "build:docker": "docker build -t agapequiz:latest .",
    "start:docker": "docker run -p 3000:3000 agapequiz:latest",
    "deploy:docker": "npm run build:docker && npm run start:docker",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:prod": "docker-compose --profile prod up --build -d",
    "docker:down": "docker-compose down",
    "build:script": "node scripts/build-production.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/material-nextjs": "^7.3.0",
    "@prisma/client": "^6.14.0",
    "blob-stream": "^0.1.3",
    "framer-motion": "^12.23.12",
    "next": "15.4.6",
    "nodemailer": "^7.0.5",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.6",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.0",
    "@types/pdfkit": "^0.17.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "9.33.0",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.14.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
